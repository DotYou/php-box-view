{
    "name": "syslogic/box-view",
    "type": "project",
    "description": "PHP SDK for the Box View API.",
    "keywords": [
        "box",
        "view",
        "sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Box View SDK Contributors",
            "homepage": "https://github.com/SyslogicNL/php-box-view/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "4.*",
        "doctrine/collections": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "BoxView\\": "src/BoxView"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoxViewTest\\": "tests/BoxView"
        }
    }
}
